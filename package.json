    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.1",
    "mongodb": "4.1",
    "mongoose": "^5.13.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.4",
    "pug": "^3.0.2",
    "request": "^2.88.2",
    "sharp": "^0.32.1",
    "slugify": "^1.6.6",
    "validator": "^13.9.0"
  }
